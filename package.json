{
  "name": "code-outline",
  "displayName": "Code Outline for Ease",
  "description": "Extract and display function headers from source files for easy documentation.",
  "version": "1.1.0",
  "publisher": "Rowan00Omar",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other",
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-outline.codeOutline",
        "title": "Outline My Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "echo 'No compile step needed'",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "compile": "tsc -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rowan00Omar/VSCode-Extension-Code-Outline.git"
  },
  "icon": "images/icon.png",
  "keywords": [
    "vscode",
    "extension",
    "function",
    "outline",
    "documentation",
    "code",
    "python",
    "webview",
    "javascript",
    "programming language"
  ],
  "author": "Your Rowan Omar <rowan00omar@gmail.com>",
  "license": "MIT"
}
